node{	
	properties(
		[
			pipelineTriggers(
				[
					pollSCM('* * * * *')
				]
			),
		]
	)
        stage('Build') {				
			checkout([$class: 'GitSCM', 				
			branches: [[name: "origin/main"]], 
			userRemoteConfigs: [[
			url: 'https://github.com/svabhi/devnewcicd.git']]])

        }		

        stage('approve') {
            input 'approve or not'
        }
}