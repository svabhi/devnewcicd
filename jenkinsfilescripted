node (label: 'Built-In Node'){
	properties(
		[
			buildDiscarder(
				logRotator(
					numToKeepStr: '1'
				)
			)
		]
	)
	
	stage('Build'){
		echo 'Hello World'
	}
}

def jenkins = Jenkins.instance
def computers = jenkins.computers
computers.each {
   //  println "${it.displayName} ${it.hostName}"
}

def labels = jenkins.getLabels()
labels.each {
   println "${it.displayName}"
}